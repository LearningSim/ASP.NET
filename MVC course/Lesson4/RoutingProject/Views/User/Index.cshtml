@model IList<LessonProject.Model.User>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Users</h2>

<p>
	@foreach (var user in Model) {
		<div class="item">
			<span class="id">
				@user.Id
			</span>
			<span class="email">
				@user.Email
			</span>
			<span class="password">
				@user.Password
			</span>
		</div>
	}
</p>